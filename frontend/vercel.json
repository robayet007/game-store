{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://13.236.52.33:5000/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "http://13.236.52.33:5000/uploads/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}